<a href="#watch?v={{ idVideo }}" title="{{ title }}">
  <figure class="image">
    <img src="{{ image.url }}" alt="{{ title }}">
  </figure>
  <div class="title">
    <p>
      {{ title }}
    </p>
  </div>
</a>
<a href="https://www.youtube.com/user/{{ channelTitle }}" class="channel">{{ channelTitle }}</a>
